Water
{
	$surfaceprop Water

	$abovewater 1

	$ignorez 1
	$no_fullbright 1

	%compilewater 1
	%keywords "tf"

	$distancealpha 1
	$distancealphafromdetail 1
	$edgesoftnessend 0.3
	$edgesoftnessstart 0.45
	$glow 1
	$glowalpha 0.45
	$glowcolor "[0 0 0]"
	$glowend 0.5
	$glowstart 0.03
	$glowx -0.001
	$glowy -0.001
	$outline 1
	$outlinecolor "[0.7 0.7 0.5]"
	$outlineend0 0.49
	$outlineend1 0.73
	$outlinestart0 0.3
	$outlinestart1 0.49
	$scaleedgesoftnessbasedonscreenres 1
	$scaleoutlinesoftnessbasedonscreenres 1
	$softedges 1

	<dx90
	{
		$outlinecolor "[0.9 0.9 0.2]"
		$outlinestart0 "0.4"
		$outlinestart1 "0.525"
		$outlineend0 "0.525"
		$outlineend1 "0"
	}
}